<div class="controlsDemo__wrapper">
    <div class="controlsDemo__cell" attr:style="width: 300px">
        <div class="controls-text-label_theme-{{_options.theme}}">diagram with borderVisible=false</div>
        <Controls.process:Diagram data="{{_data}}" borderVisible="{{false}}"></Controls.process:Diagram>
    </div>
    <div class="controlsDemo__cell" attr:style="width: 300px">
        <div class="controls-text-label_theme-{{_options.theme}}">diagram with borderVisible=true</div>
        <Controls.process:Diagram data="{{_data}}" borderVisible="{{true}}"></Controls.process:Diagram>
    </div>
</div>